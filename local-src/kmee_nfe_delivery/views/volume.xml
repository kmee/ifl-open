<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="kmee_nfe_delivery_volume_form" model="ir.ui.view">
        <field name="name">kmee_nfe_delivery.volume.form</field>
        <field name="model">kmee_delivery.volumes</field>
        <field name="arch" type="xml">
            <form>
                <separator string="Dados do Volume"/>
                <group>
                    <group>
                        <field name="quantidade"/>
                        <br/>
                        <field name="especie"/>
                        <br/>
                    </group>
                    <group>
                        <field name="marca"/>
                        <br/>
                        <field name="numero_volume"/>
                    </group>
                </group>
                <group>
                    <group>
                        <field name="peso_liquido"/>
                        <field name="peso_bruto"/>
                    </group>
                </group>
                <separator string="Lacres"/>
                <field name="lacres_ids">
                    <tree editable="bottom">
                        <field name="numero_lacre"/>
                    </tree>
                </field>
            </form>
        </field>
    </record>

</odoo>